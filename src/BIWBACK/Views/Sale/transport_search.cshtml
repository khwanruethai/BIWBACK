@{
    Layout = "_Layout";
}
<section class="content-header">
    <h1>
        ใบขนส่งสินค้า > ค้นหาข้อมูล
        <small>Preview</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="#">Forms</a></li>
        <li class="active">General Elements</li>
    </ol>
</section>
<section class="content">
    <div class="form-group">
        <div class="row">
            <div class="col-sm-12">
                <div class="box box-info">
                    <div class="box-header with-border">
                        <h3 class="box-title">ค้นหาข้อมูล</h3>

                        <div class="box-tools pull-right">
                            <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                <i class="fa fa-minus"></i>
                            </button>
                        </div>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">
                        <div class="col-md-6">
                           <div class="form-group">
                               <div class="row">
                                   <div class="col-md-4">
                                       @Html.CheckBox("a1", new { @checked = true }) เลือกแสดงตามวันที่
                                   </div>
                               </div>
                           </div>
                            <div class="form-group">
                               <div class="row">
                                   <div class="col-md-2">
                                       วันที่ เริ่ม
                                   </div>
                                   <div class="col-md-4">
                                       @Html.TextBox("start", "", new { @class="form-control datepicker"})
                                   </div>
                                   <div class="col-md-2">
                                       ถึง วันที่
                                   </div>
                                   <div class="col-md-4">
                                       @Html.TextBox("end", "", new { @class = "form-control datepicker" })
                                   </div>
                               </div>
                           </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-2">
                                        เลขที่ใบส่งสินค้า
                                    </div>
                                    <div class="col-md-4">
                                        @Html.TextBox("start", "", new { @class = "form-control datepicker" })
                                    </div>
                                    <div class="col-md-2">
                                       รหัสสินค้า
                                    </div>
                                    <div class="col-md-4">
                                        @Html.TextBox("end", "", new { @class = "form-control datepicker" })
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-2">
                                        รหัสลูกค้า
                                    </div>
                                    <div class="col-md-4">
                                        @Html.TextBox("start", "", new { @class = "form-control datepicker" })
                                    </div>
                                    <div class="col-md-2">
                                        ชื่อลูกค้า
                                    </div>
                                    <div class="col-md-4">
                                        @Html.TextBox("end", "", new { @class = "form-control datepicker" })
                                    </div>
                                </div>
                            </div>
                            <hr />
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-2">
                                        เงื่อนไขการค้นหา
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-12">
                                       @Html.RadioButton("a","a",new { @checked="true"}) Vat  &nbsp;&nbsp;&nbsp;
                                        @Html.RadioButton("a", "a", new { @checked = "true" }) ไม่ Vat &nbsp;&nbsp;&nbsp;
                                        @Html.RadioButton("a", "a", new { @checked = "true" }) ทั้งหมด
